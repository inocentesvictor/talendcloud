public class Order implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private String id;
	private String notes;
	private Customer customer;
	private Orderlines orderlines;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
